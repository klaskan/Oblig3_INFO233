<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.updateFakturaController">
    <children>
        <Button layoutX="523.0" layoutY="341.0" mnemonicParsing="false" onAction="#changeScenButton" text="Back" />
        <Label layoutX="108.0" layoutY="186.0" text="Faktura id" />
        <Label layoutX="129.0" layoutY="222.0" text="Kunde" />
        <Label layoutX="134.0" layoutY="258.0" text="Dato" />
        <TextField fx:id="fakturaId" layoutX="186.0" layoutY="181.0" />
        <TextField fx:id="kunde" layoutX="186.0" layoutY="217.0" />
        <TextField fx:id="dato" layoutX="186.0" layoutY="253.0" promptText="eks: 12.12.2000" />
      <Label layoutX="60.0" layoutY="42.0" text="Oppdater Faktura" />
      <Button fx:id="kundeOppdaterKnappe" layoutX="365.0" layoutY="217.0" mnemonicParsing="false" onAction="#updateKunde" text="Oppdater" />
      <Button fx:id="datoOppdaterKnapp" layoutX="365.0" layoutY="253.0" mnemonicParsing="false" onAction="#updateDato" text="Oppdater" />
      <Label layoutX="204.0" layoutY="42.0" text="INFO: Skriv inn faktura iden du ønsker å forandre på for " />
      <Label layoutX="244.0" layoutY="59.0" text="derretter å legge inn informasjon du vil oppdatere." />
        <TableView fx:id="tableview" layoutY="-2.0" onMouseClicked="#buildData" prefHeight="140.0" prefWidth="600.0" AnchorPane.bottomAnchor="262.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-2.0" />
    </children>
